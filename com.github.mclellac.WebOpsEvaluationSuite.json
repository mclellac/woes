{
    "app-id": "com.github.mclellac.WebOpsEvaluationSuite",
    "metainfo": "data/com.github.mclellac.WebOpsEvaluationSuite.metainfo.xml.in",
    "runtime": "org.gnome.Platform",
    "runtime-version": "master",
    "sdk": "org.gnome.Sdk",
    "command": "woes",
    "finish-args": [
        "--share=network",
        "--share=ipc",
        "--socket=fallback-x11",
        "--device=dri",
        "--socket=wayland"
    ],
    "cleanup": [
        "/include",
        "/lib/pkgconfig",
        "/man",
        "/share/doc",
        "/share/gtk-doc",
        "/share/man",
        "/share/pkgconfig",
        "*.la",
        "*.a"
    ],
    "modules": [
        {
            "name": "woes",
            "builddir": true,
            "buildsystem": "meson",
            "build-options": {
                "python": "python3"
            },
            "sources": [
                {
                    "type": "git",
                    "url": "https://github.com/mclellac/woes.git",
                    "branch": "main"
                }
            ]
        }
    ],
    "runtime-dependencies": [
        "python3-requests"
    ],
    "output": "com.github.mclellac.WebOpsEvaluationSuite"
}