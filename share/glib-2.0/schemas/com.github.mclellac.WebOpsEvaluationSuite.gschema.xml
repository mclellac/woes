<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="woes">
	<schema id="com.github.mclellac.WebOpsEvaluationSuite" path="/com/github/mclellac/WebOpsEvaluationSuite/">
    <key name="font-size" type="i">
      <default>12</default>
      <range min="8" max="24"/>
      <summary>Font size</summary>
      <description>Font size in points</description>
    </key>
    <key name="dark-theme" type="b">
      <default>false</default>
      <summary>Dark theme</summary>
      <description>Enable dark theme</description>
    </key>
	</schema>
</schemalist>

