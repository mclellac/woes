<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.91.0 -->
<interface>
  <!-- interface-name window.ui -->
  <!-- interface-copyright (c) 2024 -->
  <!-- interface-authors Carey McLelland <careymclelland@gmail.com> -->
  <requires lib="gio" version="2.44"/>
  <requires lib="gtk" version="4.14"/>
  <requires lib="libadwaita" version="1.5"/>
  <template class="WoesWindow" parent="AdwApplicationWindow">
    <property name="content">
      <object class="GtkBox" id="main_box">
        <property name="orientation">1</property>
        <child>
          <object class="AdwHeaderBar" id="header_bar">
            <property name="title-widget">
              <object class="AdwViewSwitcher" id="switcher_title">
                <property name="halign">baseline-fill</property>
                <property name="policy">wide</property>
                <property name="stack">stack</property>
              </object>
            </property>
            <child type="end">
              <object class="GtkMenuButton" id="menu_button">
                <property name="icon-name">open-menu-symbolic</property>
                <property name="menu-model">primary_menu</property>
                <property name="primary">True</property>
                <property name="tooltip-text" translatable="yes">Menu</property>
                <style>
                  <class name="GtkMenuButton"/>
                </style>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="AdwViewStack" id="stack">
            <child>
              <object class="AdwViewStackPage" id="http_page">
                <property name="child">
                  <object class="AdwStatusPage" id="http_status_page">
                    <property name="description">This tool is ideal for developers, as well as systems and network administrators, who need to inspect HTTP headers for debugging, troubleshooting, and security analysis.</property>
                    <property name="halign">baseline-fill</property>
                    <property name="icon-name" bind-source="http_page" bind-property="icon-name" bind-flags="sync-create"/>
                    <property name="title" bind-source="http_page" bind-property="title" bind-flags="sync-create"/>
                    <child>
                      <object class="AdwClampScrollable" id="http_clamp_scrollable">
                        <property name="maximum-size">800</property>
                        <property name="unit">px</property>
                        <child>
                          <object class="GtkBox" id="HttpPage">
                            <child>
                              <object class="HttpPage"/>
                            </child>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </property>
                <property name="icon-name">network-transmit-receive-symbolic</property>
                <property name="name">http_page</property>
                <property name="title">HTTP Headers</property>
                <property name="use-underline">true</property>
              </object>
            </child>
            <child>
              <object class="AdwViewStackPage" id="nmap_page">
                <property name="child">
                  <object class="AdwStatusPage" id="nmap_status_page">
                    <property name="description"> This tool is designed to facilitate network scanning and reconnaissance using Nmap, a powerful network scanning tool. This page provides a user-friendly interface for performing Nmap scans and viewing their results.</property>
                    <property name="icon-name" bind-source="nmap_page" bind-property="icon-name" bind-flags="sync-create"/>
                    <property name="title" bind-source="nmap_page" bind-property="title" bind-flags="sync-create">Port Scanning (Nmap)</property>
                    <child>
                      <object class="AdwClampScrollable">
                        <child>
                          <object class="GtkBox" id="NmapPage">
                            <property name="orientation">1</property>
                            <property name="valign">3</property>
                            <child>
                              <object class="NmapPage"/>
                            </child>
                          </object>
                        </child>
                      </object>
                    </child>
                  </object>
                </property>
                <property name="icon-name">face-devilish</property>
                <property name="name">nmap_page</property>
                <property name="title">Port Scan</property>
                <property name="use-underline">true</property>
              </object>
            </child>
            <child>
              <object class="AdwViewStackPage" id="purger_page">
                <property name="badge-number">0</property>
                <property name="child">
                  <object class="AdwStatusPage" id="purger_status_page">
                    <property name="child">
                      <object class="AdwClamp" id="purger_clamp">
                        <property name="maximum-size">500</property>
                      </object>
                    </property>
                    <property name="margin-end">15</property>
                    <property name="margin-start">15</property>
                    <property name="title" bind-source="purger_page" bind-property="title" bind-flags="sync-create"/>
                    <property name="valign">1</property>
                  </object>
                </property>
                <property name="icon-name">user-trash-symbolic</property>
                <property name="name">purger_page</property>
                <property name="title" translatable="yes">Cache Purge</property>
                <property name="use-underline">true</property>
              </object>
            </child>
            <child>
              <object class="AdwViewStackPage" id="stats_page">
                <property name="child">
                  <object class="AdwStatusPage" id="stats_status_page">
                    <property name="child">
                      <object class="GtkBox" id="stats_box">
                        <property name="orientation">1</property>
                        <property name="valign">3</property>
                      </object>
                    </property>
                    <property name="icon-name" bind-source="stats_page" bind-property="icon-name" bind-flags="sync-create"/>
                    <property name="title" bind-source="stats_page" bind-property="title" bind-flags="sync-create"/>
                  </object>
                </property>
                <property name="icon-name">face-cool</property>
                <property name="name">stats_page</property>
                <property name="title" translatable="yes">Stats</property>
                <property name="use-underline">true</property>
              </object>
            </child>
          </object>
        </child>
        <child>
          <object class="AdwViewSwitcherBar" id="switcher_bar">
            <property name="halign">baseline-fill</property>
            <property name="stack">stack</property>
          </object>
        </child>
      </object>
    </property>
    <property name="default-height">1000</property>
    <property name="default-width">1450</property>
    <property name="height-request">360</property>
    <property name="title" translatable="yes">AdwViewSwitcher Demo</property>
    <property name="width-request">360</property>
    <child>
      <object class="AdwBreakpoint">
        <!-- Custom object fragments -->
        <condition>max-width: 550sp</condition>
        <setter object="header_bar" property="title-widget"/>
        <setter object="switcher_bar" property="reveal">true</setter>
      </object>
    </child>
  </template>
  <menu id="primary_menu">
    <section>
      <item>
        <attribute name="action">app.preferences</attribute>
        <attribute name="label" translatable="yes">_Preferences</attribute>
      </item>
      <item>
        <attribute name="action">win.show-help-overlay</attribute>
        <attribute name="label" translatable="yes">_Keyboard Shortcuts</attribute>
      </item>
      <item>
        <attribute name="action">app.about</attribute>
        <attribute name="label" translatable="yes">_About WebOps Evaluation Suite</attribute>
      </item>
    </section>
  </menu>
</interface>
