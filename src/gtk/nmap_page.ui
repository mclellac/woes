<?xml version='1.0' encoding='UTF-8'?>
<!-- Created with Cambalache 0.91.0 -->
<interface>
  <requires lib="gtk" version="4.12"/>
  <requires lib="libadwaita" version="1.4"/>
  <template class="NmapPage" parent="GtkBox">
    <property name="halign">baseline-fill</property>
    <property name="hexpand">True</property>
    <property name="margin-top">20</property>
    <property name="orientation">vertical</property>
    <child>
      <object class="GtkListBox" id="nmap_list_box">
        <property name="activate-on-single-click">False</property>
        <property name="hexpand">True</property>
        <property name="selection-mode">0</property>
        <property name="valign">baseline-center</property>
        <property name="vexpand">True</property>
        <child>
          <object class="AdwEntryRow" id="target_entry_row">
            <property name="input-purpose">url</property>
            <property name="show-apply-button">True</property>
            <property name="title" translatable="yes">IP/CIDR/FQDN to scan</property>
          </object>
        </child>
        <child>
          <object class="AdwSwitchRow" id="fingerprint_switch_row">
            <property name="subtitle" translatable="yes">Enable this to attempt detection of the target OS</property>
            <property name="title" translatable="yes">OS Fingerprinting (requires root)</property>
          </object>
        </child>
        <child>
          <object class="AdwSwitchRow" id="scan_all_ports_switch_row">
            <property name="subtitle">This will scan all ports between 0-65535 </property>
            <property name="subtitle-lines">2</property>
            <property name="title" translatable="yes">Scan all ports (default is the top 200)</property>
          </object>
        </child>
        <child>
          <object class="AdwComboRow" id="nmap_scripts_drop_down">
            <property name="hexpand">True</property>
            <property name="hexpand-set">True</property>
            <property name="icon-name">emblem-documents-symbolic</property>
            <property name="model">
              <object class="GtkStringList" id="nmap_list">
                <property name="strings">None</property>
                <items>
                  <item>default</item>
                  <item>vuln</item>
                  <item>discovery</item>
                  <item>broadcast</item>
                  <item>exploit</item>
                  <item>http-grep</item>
                  <item>http-enum</item>
                  <item>http-config-backup</item>
                  <item>http-wordpress-enum</item>
                  <item>http-cors</item>
                  <item>http-csrf</item>
                  <item>http-cross-domain-policy</item>
                  <item>mysql-users</item>
                  <item>vulscan/vulscan</item>
                  <item>firewalk</item>
                </items>
              </object>
            </property>
            <property name="selected">0</property>
            <property name="subtitle">Select script to use if desired</property>
            <property name="subtitle-lines">1</property>
            <property name="title">Nmap Scripts</property>
            <property name="valign">baseline-fill</property>
          </object>
        </child>
        <style>
          <class name="boxed-list"/>
        </style>
      </object>
    </child>
    <child>
      <object class="GtkGrid" id="nmap_grid">
        <property name="column-homogeneous">True</property>
        <property name="halign">baseline-center</property>
        <property name="row-homogeneous">True</property>
        <child>
          <object class="GtkSpinner" id="nmap_spinner">
            <property name="halign">start</property>
            <property name="margin-start">5</property>
            <property name="spinning">True</property>
            <property name="valign">center</property>
            <property name="visible">False</property>
            <layout>
              <property name="column">1</property>
              <property name="row">2</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkLabel" id="nmap_status">
            <property name="halign">end</property>
            <property name="justify">right</property>
            <property name="label">&lt;b&gt;&lt;i&gt;Scanning...&lt;/i&gt;&lt;/b&gt;</property>
            <property name="use-markup">True</property>
            <property name="valign">center</property>
            <property name="visible">False</property>
            <layout>
              <property name="column">0</property>
              <property name="row">2</property>
              <property name="row-span">1</property>
            </layout>
          </object>
        </child>
        <child>
          <object class="GtkLabel" id="nmap_warn_label">
            <property name="label">&lt;b&gt;WARNING:&lt;/b&gt; Do not use this tool on any hosts or networks &lt;u&gt;you do not explicitely have permission&lt;/u&gt; to scan.</property>
            <property name="margin-bottom">10</property>
            <property name="margin-end">10</property>
            <property name="margin-start">10</property>
            <property name="margin-top">10</property>
            <property name="natural-wrap-mode">word</property>
            <property name="use-markup">True</property>
            <property name="width-chars">30</property>
            <property name="wrap">True</property>
            <layout>
              <property name="column">0</property>
              <property name="column-span">2</property>
              <property name="row">0</property>
              <property name="row-span">2</property>
            </layout>
          </object>
        </child>
      </object>
    </child>
    <child>
      <object class="GtkFrame" id="nmap_frame">
        <property name="halign">baseline-fill</property>
        <property name="margin-top">5</property>
        <property name="valign">baseline-fill</property>
        <property name="vexpand">True</property>
        <child>
          <object class="GtkColumnView" id="scan_column_view">
            <property name="halign">baseline-fill</property>
            <property name="hexpand">True</property>
            <property name="margin-bottom">10</property>
            <property name="margin-end">10</property>
            <property name="margin-start">10</property>
            <property name="margin-top">10</property>
            <property name="show-column-separators">true</property>
            <property name="show-row-separators">true</property>
            <property name="valign">baseline-fill</property>
            <child>
              <object class="GtkColumnViewColumn" id="target_col">
                <property name="factory">
                  <object class="GtkSignalListItemFactory"/>
                </property>
                <property name="fixed-width">0</property>
                <property name="title" translatable="yes">Target</property>
                <property name="visible">False</property>
              </object>
            </child>
            <child>
              <object class="GtkColumnViewColumn" id="results_col">
                <property name="expand">True</property>
                <property name="factory">
                  <object class="GtkSignalListItemFactory"/>
                </property>
                <property name="fixed-width">0</property>
                <property name="title">Results</property>
                <property name="visible">False</property>
              </object>
            </child>
          </object>
        </child>
      </object>
    </child>
  </template>
</interface>
